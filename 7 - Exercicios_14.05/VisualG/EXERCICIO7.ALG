Algoritmo "exercicio7"

Var
   idade: inteiro


Inicio
   escreva("Digite a idade: ")
   leia(idade)

   escolha idade
   caso 1 até 10
      escreval("Valor: R$ 80,00")
   caso 11 até 40
      escreval("Valor: R$ 50,00")
   caso 41 até 60
      escreval("Valor: R$ 95,00")
   outrocaso
      escreval("Valor: R$ 130,00")
   fimescolha

Fimalgoritmo