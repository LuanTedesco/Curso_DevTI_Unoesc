Algoritmo "exercicio5"

Var
   letra: caracter

Inicio
   escreval("Digite uma letra: ")
   leia(letra)

   escolha letra
   caso "A"
      escreval("Vogal")
   caso "E"
      escreval("Vogal")
   caso "I"
      escreval("Vogal")
   caso "O"
      escreval("Vogal")
   caso "U"
      escreval("Vogal")
   outrocaso
      escreval("Consoante")
   fimescolha

Fimalgoritmo