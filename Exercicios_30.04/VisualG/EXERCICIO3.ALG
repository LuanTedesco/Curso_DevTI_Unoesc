Algoritmo "exercicio3"

Var
   num1, num2: inteiro
   
Inicio
   escreval("Digite o primeiro numero: ")
   leia (num1)
   
   escreval("Digite o segundo numero: ")
   leia (num2)

   se num1 > num2 entao
      escreval("O numero", num1, " é maior que o numero", num2)
   senao
      escreval("O numero", num2, " é maior que o numero", num1)
   fimse

Fimalgoritmo