Algoritmo "exercicio4"

Var
   media: real


Inicio
   escreva("Digite a media do aluno: ")
   leia(media)

   escolha media
   caso 0 até 4
      escreval("F")
   caso 5
      escreval("E")
   caso 6
      escreval("D")
   caso 7
      escreval("C")
   caso 8
      escreval("B")
   caso 9 até 10
      escreval("A")
   outrocaso
      escreval("Nota inválida")
   fimescolha

Fimalgoritmo