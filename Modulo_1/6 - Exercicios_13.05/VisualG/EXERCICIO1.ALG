Algoritmo "exercicio1"

Var
   codigo: inteiro


Inicio
   escreval("Digite o código: ")
   leia(codigo)

   escolha codigo
   caso 1
      escreval("Alimento não-perecível")
   caso 2
      escreval("Alimento perecivel")
   caso 3
      escreval("Vestuário")
   caso 4
      escreval("Limpeza")
   outrocaso
      escreval("Código inválido")
   fimescolha
   




Fimalgoritmo